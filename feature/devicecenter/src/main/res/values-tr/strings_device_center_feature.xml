    <string name="device_center_top_app_bar_search_devices_hint">Arama cihazları</string>
    <!-- The hint text for the search bar in the Device Center feature when the User is searching for Syncs -->
    <string name="device_center_top_app_bar_search_syncs_hint">Arama senkronizasyonları</string>
