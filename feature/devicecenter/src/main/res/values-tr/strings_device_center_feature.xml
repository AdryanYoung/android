    <string name="device_center_device_top_menu_option_rename">Yeniden Adlandır</string>
    <!-- Label of the top app bar menu option of selected device in Device Center which takes the user to the device info screen -->
    <string name="device_center_device_top_menu_option_info">Bilgi</string>
